{
    "manifest_version": 3,
    "name": "More YouTube Shortcuts",
    "version": "0.1.0",
    "description": "More keyboard shortcuts for skipping ahead 30 seconds, liking & disliking videos, & more",
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/watch?*"],
            "js": ["script.js"]
        }
    ],
    "icons": {
        "16": "logo_16.png",
        "48": "logo_48.png",
        "128": "logo_128.png"
    }
}